{
    "name": "denisyukphp/tmpfile",
    "description": "Alternative to tmpfile() function",
    "keywords": ["tmpfile", "tempnam", "temp", "tmp", "file"],
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksandr Denisyuk",
            "email": "a@denisyuk.by",
            "homepage": "https://denisyuk.by",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "TmpFile\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TmpFile\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "denisyukphp/tmpfile-manager": "Allows more advanced use temp files"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}